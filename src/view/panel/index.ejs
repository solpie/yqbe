<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Panel</title>
    <style>
        body {
            overflow-x: hidden;
            overflow-y: hidden;
            margin: 0;
        }

        #op {
            position: fixed;
            z-index: 2;
            left: 0;
            top: 0;
        }

        #ex {
            position: fixed;
            z-index: 3;
            left: 0;
            top: 0;
        }
        .PlayerIntro{
            position: absolute;
            overflow: hidden;
            display: none;
            width: 230px;
            height: 230px;
            border: 0;
            color: #ddd;
            font-size: 18px;
        }
        .BlueIntro {
            left: 437px;
            top: 565px;
        }
        .RedIntro {
            left: 1270px;
            top: 565px;
        }
    </style>
    <link type="text/css" rel="stylesheet" href="/css/materialize.min.css" media="screen,projection"/>
</head>
<body>
<div id="panel">
    <canvas id="stage"></canvas>
    <div id="op">
        <router-view></router-view>
    </div>
    <div id="ex">

    </div>
</div>
<script type="text/javascript">
    var host = '<%= host %>'
    var wsPort = '<%= wsPort %>'
    console.log('host', host, 'wsPort', wsPort);
</script>
<script src="/js/jquery.min.js"></script>
<script src="/js/materialize.min.js"></script>
<script src="/js/createjs/createjs.min.js"></script>
<script src="/js/createjs/easeljs.min.js"></script>
<script src="/js/createjs/tweenjs.min.js"></script>
<script src="/js/socket.io-1.4.5.min.js"></script>
<script src="/panel/index.js"></script>
</body>
</html>