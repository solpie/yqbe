  "scripts": {
    "watch": "npm run server&npm run view",
    "build": "npm run server-build&npm run view-build",
    "view": "webpack --watch --progress --profile --colors --disZplay-error-details --display-cached --config webpack.config.view.js",
    "view-build": "webpack --progress --profile --colors --disZplay-error-details --display-cached --config webpack.config.view.js",
    "server": "webpack --watch --progress --profile --colors --display-error-details --display-cached --config webpack.config.server.js",
    "server-build": "webpack --progress --profile --colors --display-error-details --display-cached --config webpack.config.server.js",
    "pack": "npm run electron-packager&&npm run copy-node_modules&&explorer YuanQiTV-Billiard-win32-x64",
    "app": "npm run build && electron app --enable-logging",
    "electron-packager": "electron-packager app YuanQiTV-Billiard --ignore=node_modules/electron-* --platform=win32 --arch=x64 --overwrite",
    "copy-node_modules": "echo D|xcopy node_modules YuanQiTV-Billiard-win32-x64\\resources\\node_modules /EXCLUDE:node_modules.ignore /e /y",
    "copy-app": "node script/copy_app.js",
    "dtsm": "dtsm init",
    "dl": "npm install electron-prebuilt -g"
  },
  "dependencies": {
    "ansi2html": "0.0.1",
    "body-parser": "^1.15.1",
    "colors": "^1.1.2",
    "cropper": "^2.3.1",
    "crypto": "0.0.3",
    "ejs": "^2.4.2",
    "express": "^4.13.4",
    "morgan": "^1.7.0",
    "msgpack-lite": "^0.1.20",
    "nedb": "^1.8.0",
    "sequent": "^0.1.2",
    "socket.io": "^1.4.6",
    "vue": "^1.0.10",
    "vue-class-component": "^3.2.0",
    "vue-resource": "^0.7.2",
    "vue-router": "^0.7.7",
    "xlsx": "^0.8.0"
  },
  "devDependencies": {
    "coffee-loader": "^0.7.2",
    "coffee-script": "^1.10.0",
    "copy-webpack-plugin": "^3.0.1",
    "electron-prebuilt": "^1.2.0",
    "html-loader": "^0.3.0",
    "source-map-loader": "^0.1.5",
    "ts-loader": "^0.7.1",
    "tsd": "^0.6.5",
    "typescript": "^1.8.9",
    "webpack": "^1.12.6",
    "webpack-node-externals": "^1.2.0"
  }
